<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.1</TargetFramework>
    <TypeScriptToolsVersion>3.0</TypeScriptToolsVersion>
    <ApplicationIcon />
    <OutputType>Exe</OutputType>
    <StartupObject>KurnikNET.Engine.EngineDeployAction</StartupObject>
    <OutputPath>bin</OutputPath>
    <DebugType>none</DebugType>
    <DebugSymbols>false</DebugSymbols>
    <TypeScriptTarget>ES5</TypeScriptTarget>
    <TypeScriptJSXEmit>None</TypeScriptJSXEmit>
    <TypeScriptModuleKind>AMD</TypeScriptModuleKind>
    <TypeScriptCompileOnSaveEnabled>True</TypeScriptCompileOnSaveEnabled>
    <TypeScriptNoImplicitAny>False</TypeScriptNoImplicitAny>
    <TypeScriptRemoveComments>True</TypeScriptRemoveComments>
    <TypeScriptOutFile>bin\game_engine.js</TypeScriptOutFile>
    <TypeScriptOutDir></TypeScriptOutDir>
    <TypeScriptGeneratesDeclarations>False</TypeScriptGeneratesDeclarations>
    <TypeScriptNoEmitOnError>True</TypeScriptNoEmitOnError>
    <TypeScriptSourceMap>False</TypeScriptSourceMap>
    <TypeScriptMapRoot />
    <TypeScriptSourceRoot></TypeScriptSourceRoot>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Source\engine\font.ts" />
    <None Remove="Source\engine\material.ts" />
    <None Remove="Source\engine\texture.ts" />
    <None Remove="Source\geometry\2d\text2d.ts" />
    <None Remove="Source\net\http_request.ts" />
    <None Remove="Source\net\http_response.ts" />
    <None Remove="Source\net\index.ts" />
    <None Remove="Source\shaders\geometry.ts" />
    <None Remove="Source\shaders\lighting.ts" />
  </ItemGroup>

  <ItemGroup>
    <TypeScriptCompile Include="Source\engine\font.ts" />
    <TypeScriptCompile Include="Source\engine\material.ts" />
    <TypeScriptCompile Include="Source\engine\texture.ts" />
    <TypeScriptCompile Include="Source\game_engine.ts" />
    <TypeScriptCompile Include="Source\geometry\2d\text2d.ts" />
    <TypeScriptCompile Include="Source\net\http_request.ts" />
    <TypeScriptCompile Include="Source\net\http_response.ts" />
    <TypeScriptCompile Include="Source\net\index.ts" />
    <TypeScriptCompile Include="Source\shaders\geometry.ts" />
    <TypeScriptCompile Include="Source\shaders\lighting.ts" />
  </ItemGroup>

</Project>
